<resources>
    <properties>
        <property id="username" type="string"></property>
        <property id="password" type="string"></property>
    </properties>

    <strings>
       <string id="username">Tesla user ID</string>
       <string id="password">Tesla password</string>
    </strings>

    <settings>
        <setting propertyKey="@Properties.username" title="@Strings.username">
            <settingConfig type="email" required="true" />
        </setting>
        <setting propertyKey="@Properties.password" title="@Strings.password">
            <settingConfig type="password" required="true" />
        </setting>
    </settings>
</resources>
